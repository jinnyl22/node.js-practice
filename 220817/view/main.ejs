        <label for="">아이디</label> // 123
        <input type="text" id="id" />
        <label for="">비밀번호</label> // 456
        <input type="text" id="pw" />
        <button onclick="join()">회원가입</button>
</body>
<script>
    // 위의 
    function join(){
        $.ajax({
            url : "/signup",
            type : "post",
            data : {
                id : document.querySelector("#id").value,
                pw : document.querySelector("#pw").value
            },
            
        })
    }
</script>
